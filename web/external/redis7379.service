[Unit]
Description=Redis on port 7379 for webapi
After=network.target

[Service]
ExecStart=/usr/bin/redis-server /etc/redis/redis_7379.conf --supervised systemd
ExecStop=/bin/kill -s QUIT $MAINPID
ExecReload=/bin/kill -s HUP $MAINPID
Type=notify
User=redis
Group=redis
RuntimeDirectory=redis
RuntimeDirectoryMode=0755
LimitNOFILE=65536

[Install]
WantedBy=multi-user.target

