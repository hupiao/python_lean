[Unit]
Description=Redis on port 7380 for back
After=network.target

[Service]
ExecStart=/usr/bin/redis-server /etc/redis/redis_7380.conf --supervised systemd
ExecStop=/bin/kill -s QUIT $MAINPID
ExecReload=/bin/kill -s HUP $MAINPID
Type=notify
User=redis
Group=redis
RuntimeDirectory=redis
RuntimeDirectoryMode=0755

[Install]
WantedBy=multi-user.target

